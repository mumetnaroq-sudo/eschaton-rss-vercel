{
  "version": 2,
  "name": "eschaton-rss",
  "builds": [
    {
      "src": "api/feed.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/feed.xml",
      "dest": "/api/feed.py"
    },
    {
      "src": "/api/feed",
      "dest": "/api/feed.py"
    }
  ],
  "env": {
    "MOLTHUB_SUBMOLT_ID": "11a42d04-e060-4544-a1b8-bee08f7b15ab"
  }
}
