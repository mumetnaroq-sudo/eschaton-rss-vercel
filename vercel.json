{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/feed.xml",
      "dest": "/api/index.py"
    },
    {
      "src": "/feed",
      "dest": "/api/index.py"
    },
    {
      "src": "/rss",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/feed",
      "dest": "/api/index.py"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  }
}
